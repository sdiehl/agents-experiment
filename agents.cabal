-- Initial agents.cabal generated by cabal init.  For further 
-- documentation, see http://haskell.org/cabal/users-guide/

name:                agents
version:             0.1.0.0
-- license-file:        LICENSE
author:              Stephen Diehl
maintainer:          stephen.m.diehl@gmail.com
build-type:          Simple
extra-source-files:  README.md, logo
cabal-version:       >=1.10

executable agents
  main-is: Main.hs
  hs-source-dirs:      src
  default-language:    Haskell2010
  other-extensions:    TemplateHaskell, NoMonomorphismRestriction
  build-depends:       
    base             >= 4.6 && <4.7,
    lens-family-core >= 1.0 && <1.1,
    lens-family-th   >= 0.2 && <0.3,
    containers       >= 0.5 && <0.6,
    pretty-show      >= 1.6 && <1.7,
    mtl              >= 2.1 && <2.2,
    process          >= 1.1 && <1.2,
    haskeline        >= 0.7 && <0.8,
    parsec           >= 3.1 && <3.2,
    indents          >= 0.3 && <0.4,
    errors           >= 1.4 && <1.5
